FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
transportModel  Newtonian;
// Diffusion coefficient [m2/s] 
DT              5E-07;
// Laminar viscosity
nu              5e-07;
// Wall temperature
Tw							450;
// Inlet bulk temperature
Tbin            300;




